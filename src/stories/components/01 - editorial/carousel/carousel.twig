{{ attach_library('coba/carousel') }}

<div id="carousel{{ id }}" class="carousel-single carousel slide" data-bs-ride="carousel" >
  <div class="carousel-inner">
    {% block carousel_items %}
      {% for item in list %}
        {%
          include '@coba/01 - editorial/carousel/carousel-item.twig' with {
            "id": id,
            "item": item
          } only
        %}
      {% endfor %}
    {% endblock %}
  </div>
</div>
